#include <stdio.h>
 //grading using if else if ladder
main()
{
    printf("Enter marks of student \n");
    float a;
    scanf("%f",&a);
     if (a>=85)
     { printf("GRADE A");}
      else if(a>=70)
      {printf("GRADE B");}
      else if(a>=55)
      {printf("GRADE C");}
      else if(a>=40)
      {printf("GRADE D");}
    else
          {printf("GRADE F");}
    
    return 0;
}
